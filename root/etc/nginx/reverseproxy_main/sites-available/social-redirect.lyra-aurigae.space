# Youtube -> youtube, yt
server {
    listen 443 ssl;
    server_name youtube.lyra-aurigae.space yt.lyra-aurigae.space;

    # Redirection
    include /etc/nginx/conf.d/reverseproxy_main_include_ssl.conf;
    return 301 https://www.youtube.com/channel/UCtBNrHtGO4RxraEwu0fQpAw;
}

